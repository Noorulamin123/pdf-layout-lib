{
  "type": "column",
  "children": [
    {
      "type": "table",
      "field_map": [
        {
          "label": "Financial Data",
          "group": true,
          "children": [
            {
              "label": "Ticker\nSymbol",
              "key": "Ticker"
            },
            {
              "label": "RIC\nCode",
              "key": "RIC"
            },
            {
              "label": "Price",
              "group": true,
              "children": [
                {
                  "label": "Ask\nPrice",
                  "key": "Ask",
                  "transform": "dollarize"
                },
                {
                  "label": "Bid\nPrice",
                  "key": "Bid",
                  "transform": "dollarize"
                },
                {
                  "label": "Last\nPrice",
                  "key": "Last",
                  "transform": "dollarize"
                }
              ]
            },
            {
              "label": "Volume\nTraded",
              "key": "Volume1|Volume2|Volume3|Volume4|Volume5",
              "transform": "join_lines"
            },
            {
              "label": "Exchange\nName",
              "key": "Exchange"
            },
            {
              "label": "Currency",
              "key": "Currency"
            }
          ]
        }
      ],
      "style": {
        "col_widths": [80, 60, 60, 60, 60, 80, 80, 60],
        "font_name": "Helvetica",
        "font_size": 9,
        "font_style": "bold-italic",
        "body_font_size": 9,
        "grid": true,
        "header_background": "grey",
        "header_text_color": "whitesmoke",
        "body_background": "beige"
      }
    },
    {
      "type": "separator",
      "length": 0,
      "thickness": 2,
      "color": "blue",
      "direction": "horizontal",
      "margin_before": 20,
      "margin_after": 20,
      "dash": [3,2]
    }
  ]
}